    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify-cors": "^6.0.2",
    "graphql": "15.6.1",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  }
