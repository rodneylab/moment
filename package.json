{
  "name": "moment",
  "version": "0.0.1",
  "description": "moment",
  "main": "src/server.js",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --no-notify src/server.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "gen": "prisma migrate dev --name init",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "prettier:check": "prettier --config .prettierrc --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "node dist/server.js",
    "test": "uvu -r ts-node/register tests",
    "watch": "tsc -w",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/cookie": "^8.0.0",
    "@fastify/redis": "^6.0.0",
    "@fastify/session": "^9.0.0",
    "@prisma/client": "^4.2.1",
    "argon2": "^0.28.7",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "fastify": "^4.4.0",
    "fastify-plugin": "^4.2.0",
    "graphql-scalars": "^1.18.0",
    "mercurius": "^10.1.0",
    "nexus": "^1.3.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "u2f": "^0.1.3",
    "u2f-api": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.3",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "class-validator": "^0.13.2",
    "commitlint": "^17.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "graphql": "16.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6"
  }
}
