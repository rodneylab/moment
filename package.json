{
  "name": "moment",
  "version": "0.0.1",
  "description": "moment",
  "main": "src/server.js",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon dist/server.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "gen": "prisma migrate dev --name init",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "prettier:check": "prettier --config .prettierrc --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "node dist/server.js",
    "start:ts": "ts-node src/server.ts",
    "test": "uvu -r ts-node/register tests",
    "watch": "tsc -w",
    "prepare": "husky install"
  },
  "dependencies": {
