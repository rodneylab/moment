    "argon2": "^0.28.4",
    "crypto-js": "^4.1.1",
    "fastify": "^3.27.1",
    "fastify-cookie": "^5.5.0",
    "fastify-plugin": "^3.0.1",
    "fastify-postgres": "^3.6.0",
    "fastify-redis": "^4.3.3",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "u2f": "^0.1.3",
    "u2f-api": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.0.0",
