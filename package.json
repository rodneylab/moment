    "@prisma/client": "^4.1.0",
    "apollo-server-core": "^3.10.0",
    "apollo-server-fastify": "^3.10.0",
    "apollo-server-plugin-base": "^3.6.2",
    "argon2": "^0.28.7",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "fastify": "^4.3.0",
    "fastify-plugin": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "u2f": "^0.1.3",
    "u2f-api": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.3",
